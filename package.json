{
  "name": "motion-core-monorepo",
  "packageManager": "bun@1.3.3",
  "private": true,
  "scripts": {
    "build": "bun run build:package && bun run build:docs",
    "build:docs": "bun run --cwd=apps/docs build",
    "dev": "bun run --cwd=apps/docs dev",
    "check": " bun run --cwd=apps/docs check"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
